@import "../../utils/css/iconfont.wxss";
@import '../../utils/css/remove.wxss';
page{
  background:#FAFAFA;

}
input{font-size:15px;}
.user-box{
  line-height:42px;
  box-shadow:0px 3px 6px 0px rgba(0,0,0,0.12);
  color:#666;
  margin-bottom:15px;
  background:#fff;
}
.user{
  display: inline-block;
  border-right:2px dotted #999;
  width:40px;
  font-size:18px;
  background:#fff;
  text-align: center;
  line-height:26px;
  margin-right:15px;
}
.site-box{
  -webkit-border-radius:10px;
  border-radius: 10px;
   box-shadow:0px 0px 12px 0px rgba(0,0,0,0.1);
   font-size:15px;
   background:#fff;
}
.site-box>.title{
  border-bottom:1px solid rgba(0,0,0,0.1);
}
.site-box>navigator{
  padding-left:20px;
}
.navigatorbottom{
  /* padding-bottom:10px; */
}
.flex>.yuan-green,.yuan-red{
  margin-right:20px;
}
.yuan-green+ input,.yuan-red + input{
  width:100%;
  height:50px;
}
.yuan-green+ input{
  border-bottom:1px solid #ddd;
}
.yuan-green+.input-height,.yuan-red +.input-height{
  height:40px;
  color:#666;
}
.width90{
  width:90%;
}
.cart-box{
  padding:15px;
  margin-top:10px;
  -webkit-border-radius:10px;
  border-radius:10px;
  text-align:center;
  font-size:15px;
  color:#333;
  background:#fff;
  -webkit-box-shadow:0px 3px 12px 0px rgba(0,0,0,0.12);
  box-shadow:0px 3px 12px 0px rgba(0,0,0,0.12);

}
.cart-box>view{
  margin-bottom:15px;
}
.pl20{
  padding-left:20px;
}




























.yuan-green,.yuan-red{
  border:2px solid #02BC28;
  -webkit-border-radius:50%;
  border-radius: 50%;
  width:6px;
  height:6px;
}
.yuan-red{
  border-color: red;
}