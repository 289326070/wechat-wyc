/* pages/CommonModule/CartData/CartData.wxss */
@import "../../../utils/css/iconfont.wxss";

.flexCart {
  -webkit-display: flex;
  display: flex;
  align-items: center;
}

.cart-data {
  background: #fff;
  justify-content: space-between;
  color:#666666;
  font-size:12px;
  border-bottom:1px solid rgba(231,231,231,.5);
  padding:15px;
  border-radius:10px 10px 0 0 ;
}

.name{
  font-size: 16px;
  color:#333;
}
.phone-img {
  width: 74rpx;
  height: 74rpx;
}

.cart-img {
  width: 70px;
  height: 70px;
  margin-right:10px;
  -webkit-border-radius: 50%;
  border-radius: 50%;
}

.name-box>view{
  margin-bottom:5px;
}
.icon-xing1 {
  color: #fd503b;
  font-size: 8px;
}

.cart-license {
  display: inline-block;
  background: rgba(243, 243, 243, 1);
  border: 1px solid rgba(228, 228, 228, 1);
  color:rgba(102,102,102,1);
  text-align: center;
  font-size: 12px;
  padding:2px 4px;
  -webkit-border-radius: 2px;
  border-radius: 2px;
}
.icon-xing1{
  margin:0 4px;
}